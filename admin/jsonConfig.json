        "_options": {
            "type": "panel",
            "label": "options",
            "icon": "data:image/svg+xml;base64,",
            "items": {
                "source": {
                    "type": "select",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "options": [
                        {
                            "label": "iCal-Instance",
                            "value": "ical"
                        },
                        {
                            "label": "Webservice (Jumomind)",
                            "value": "api-jumomind"
                        },
                        {
                            "label": "Webservice (abfall.io / Abfall+)",
                            "value": "api-abfallio"
                        },
                        {
                            "label": "Webservice (portal.awido.de / Awido)",
                            "value": "api-awido"
                        }
                    ],
                    "default": "ical",
                    "label": "Data source"
                },
                "globaloffset": {
                    "newLine": true,
                    "type": "number",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "global offset",
                    "help": "global offset help"
                },
                "nextseparator": {
                    "newLine": true,
                    "type": "text",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "trim": false,
                    "label": "next separator",
                    "help": "next separator help"
                },
                "skipsamedayathour": {
                    "type": "number",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "skip same day at hour",
                    "help": "skip same day at hour help",
                    "min": 0,
                    "max": 23,
                    "step": 1
                },
                "daysuntilaction": {
                    "newLine": true,
                    "type": "number",
                    "min": 0,
                    "max": 2,
                    "step": 1,
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "days until action",
                    "help": "days until action help"
                },
                "_headerOptionsSupport": {
                    "newLine": true,
                    "type": "header",
                    "text": "Support this project",
                    "size": 3,
                    "sm": 12,
                    "md": 12,
                    "lg": 12
                },
                "_ioBrokerKursBanner": {
                    "newLine": true,
                    "type": "staticImage",
                    "tooltip": "ioBroker Master Kurs",
                    "noTranslation": true,
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "href": "https://haus-automatisierung.com/iobroker-kurs/?refid=iobroker-trashschedule",
                    "src": "https://haus-automatisierung.com/images/ads/ioBroker-Kurs.png"
                }
            }
        },
        "_sourceIcal": {
            "hidden": "data.source !== 'ical'",
            "type": "panel",
            "label": "ical settings",
            "icon": "data:image/svg+xml;base64,",
            "items": {
                "ical": {
                    "type": "instance",
                    "adapter": "ical",
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "ical instance",
                    "help": "ical instance help"
                }
            }
        },
        "_sourceApiJumomind": {
            "hidden": "data.source !== 'api-jumomind'",
            "type": "panel",
            "label": "Webservice (Jumomind)",
            "icon": "data:image/svg+xml;base64,",
            "items": {
                "apiJumomindProvider": {
                    "type": "autocompleteSendTo",
                    "command": "getApiProviders",
                    "jsonData": "{\"source\": \"api-jumomind\"}",
                    "freeSolo": false,
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "Provider"
                },
                "_apiJumomindLink": {
                    "type": "staticLink",
                    "href": "http://jumomind.com/",
                    "label": "jumomind.com",
                    "icon": "data:image/svg+xml;base64,",
                    "button": true,
                    "variant": "contained",
                    "sm": 12,
                    "md": 6,
                    "lg": 6
                },
                "apiJumomindCityId": {
                    "newLine": true,
                    "hidden": "!data.apiJumomindProvider || data.apiJumomindProvider === 'err'",
                    "type": "autocompleteSendTo",
                    "command": "getApiCities",
                    "jsonData": "{\"source\": \"api-jumomind\", \"provider\": \"${data.apiJumomindProvider}\"}",
                    "freeSolo": false,
                    "alsoDependsOn": [
                        "apiJumomindProvider"
                    ],
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "City"
                },
                "_apiJumomindTypes": {
                    "hidden": "!data.apiJumomindProvider || data.apiJumomindProvider === 'err' || !data.apiJumomindCityId || data.apiJumomindCityId === 'err'",
                    "type": "textSendTo",
                    "container": "text",
                    "copyToClipboard": true,
                    "command": "getApiTypesText",
                    "jsonData": "{\"source\": \"api-jumomind\", \"provider\": \"${data.apiJumomindProvider}\", \"cityId\": \"${data.apiJumomindCityId}\"}",
                    "alsoDependsOn": [
                        "apiJumomindProvider",
                        "apiJumomindCityId"
                    ],
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "Available types"
                },
                "apiJumomindStreetId": {
                    "newLine": true,
                    "hidden": "!data.apiJumomindProvider || data.apiJumomindProvider === 'err' || !data.apiJumomindCityId || data.apiJumomindCityId === 'err'",
                    "type": "autocompleteSendTo",
                    "command": "getApiStreets",
                    "jsonData": "{\"source\": \"api-jumomind\", \"provider\": \"${data.apiJumomindProvider}\", \"cityId\": \"${data.apiJumomindCityId}\"}",
                    "freeSolo": false,
                    "alsoDependsOn": [
                        "apiJumomindProvider",
                        "apiJumomindCityId"
                    ],
                    "sm": 12,
                    "md": 6,
                    "lg": 6,
                    "label": "Street"
                }
            }
        },
